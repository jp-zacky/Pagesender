{
  "manifest_version": 2,
  "name": "Pagesender",
  "description": "page send",
  "version": "1.0",
  "background": {
        "scripts": ["./js/background.js"]
    },
    "permissions": [
        "tabs",
        "clipboardWrite"
    ],
    "commands": {
        "nextPage": {
            "suggested_key": {
                "default": "Alt+N"
            },
            "description": "next URL"
        },
        "backPage": {
            "suggested_key": {
                "default": "Alt+B"
            },
            "description": "back URL"
        }
    }
}